alias stag="rlwrap ssh deploy@oomph-staging.mogeneration.com 'source /etc/profile && cd /data/oompfserver/current && bundle exec script/console'"
alias integ="rlwrap ssh deploy@oomph-integration.mogeneration.com 'source /etc/profile && cd /data/oompfserver/current && bundle exec script/console'"

