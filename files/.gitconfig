[alias]
  ci = commit
  st = status
  co = checkout
  l   = "!source ~/.githelpers && pretty_git_log"
  lo  = !git -l --all
[user]
  name  = Jamie Cook
  email = jamie.cook@veitchlister.com.au
[color]
  diff = auto
  status = auto
  branch = auto
[core]
  excludesfile = ~/.gitignore
  autocrlf = input
[github]
  user = jamiecook
[push]
  default = current 
[format]
  pretty = oneline
